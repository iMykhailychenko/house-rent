{
    "name": "backend",
    "version": "1.0.0",
    "main": "dist/app.js",
    "license": "MIT",
    "scripts": {
        "clean": "rm -rf dist/*",
        "migration:create": "typeorm migration:create -n HouseRent",
        "migration:run": "typeorm migration:run",
        "lint": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\" --config ./.prettierrc && eslint --fix ./ && tslint -c tslint.json -p tsconfig.json --fix",
        "start": "yarn run clean && yarn run tsc && node dist/app.js",
        "dev": "nodemon --watch src --exec yarn run start",
        "test": "jest src/* --testTimeout=20000"
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ]
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "pg": "^8.7.1",
        "typeorm": "^0.2.36"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-proposal-decorators": "^7.14.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-typescript": "^7.15.0",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/fs-extra": "^9.0.12",
        "@types/jest": "^26.0.24",
        "@types/jsonwebtoken": "^8.5.4",
        "@types/morgan": "^1.9.3",
        "@types/node": "^16.4.13",
        "@types/pg": "^8.6.1",
        "@types/sequelize": "^4.28.10",
        "@types/shelljs": "^0.8.9",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^4.29.0",
        "@typescript-eslint/parser": "^4.29.0",
        "babel-jest": "^27.0.6",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "jest": "^27.0.6",
        "nodemon": "^2.0.12",
        "prettier": "^2.3.2",
        "supertest": "^6.1.4",
        "ts-node": "^10.1.0",
        "tslint": "^6.1.3",
        "typescript": "^4.3.5"
    }
}
