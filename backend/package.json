{
    "name": "backend",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "dev": "nodemon",
        "start:prod": "node ./dist/main.js",
        "start:debug": "nest start --debug --watch",
        "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/db.config.ts",
        "db:drop": "yarn typeorm schema:drop",
        "db:create": "yarn typeorm migration:generate -n",
        "db:migrate": "yarn typeorm migration:run"
    },
    "dependencies": {
        "@nestjs/axios": "^0.0.3",
        "@nestjs/common": "^7.6.15",
        "@nestjs/core": "^7.6.15",
        "@nestjs/mapped-types": "*",
        "@nestjs/platform-express": "^7.6.15",
        "@nestjs/platform-socket.io": "^8.1.1",
        "@nestjs/typeorm": "^8.0.2",
        "@nestjs/websockets": "^8.1.1",
        "@types/bcrypt": "^5.0.0",
        "bcrypt": "^5.0.1",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.3",
        "pg": "^8.7.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7",
        "typeorm": "^0.2.38"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.6.0",
        "@nestjs/schematics": "^7.3.0",
        "@nestjs/testing": "^7.6.15",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.22",
        "@types/multer": "^1.4.7",
        "@types/node": "^14.14.36",
        "@types/pg": "^8.6.1",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "eslint": "^7.22.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^3.3.1",
        "jest": "^26.6.3",
        "nodemon": "^2.0.13",
        "prettier": "^2.2.1",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.4",
        "ts-loader": "^8.0.18",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
